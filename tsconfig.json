{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": [
    "node_modules",
    "src/implementation-orchestrator.ts",
    "src/quality-enhancement-agent.ts",
    "src/services/MGAOrchestratorBackup.ts",
    "src/services/MGAOrchestrator.ts",
    "src/services/MGAOrchestrator_fixed.ts",
    "src/utils/enhanced-error-handling-broken.ts",
    "src/utils/enhanced-performance-monitoring.ts",
    "src/agents",
    "src/controls",
    "src/engines",
    "src/registry",
    "src/test",
    "src/run-soc2-agents.ts",
    "src/run-soc2-agents-updated.ts",
    "src/step-by-step-executor.ts",
    "src/quick-quality-enhancer.ts",
    "src/test-enhanced-agents.ts"
  ]
}
