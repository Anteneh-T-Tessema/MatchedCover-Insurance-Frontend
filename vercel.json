{
  "version": 2,
  "framework": "nextjs",
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "DISABLE_NEXT_FONT": "1",
      "SKIP_FONT_OPTIMIZATION": "1"
    }
  },
  "env": {
    "NEXT_PUBLIC_GEMINI_API_KEY": "@gemini_api_key"
  }
}
