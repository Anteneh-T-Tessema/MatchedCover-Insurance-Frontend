{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "npm install --legacy-peer-deps",
  "build": {
    "command": "next build",
    "env": {
      "SKIP_FONT_OPTIMIZATION": "1",
      "DISABLE_NEXT_FONT": "1"
    }
  },
  "env": {
    "NEXT_PUBLIC_GEMINI_API_KEY": "@gemini_api_key"
  }
}
